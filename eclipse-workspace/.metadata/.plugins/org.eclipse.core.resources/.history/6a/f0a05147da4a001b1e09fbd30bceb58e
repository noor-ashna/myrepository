package java8;


// final classes= String, integer, Scanner ,System, Long
public class Jocker {

	private int heyyy=100;
	private static int mxmx=90;
	
	public static void main(String[] args) {
		
		int pk=199;
		pk++;
		int gk=100;  
		final int amoga=200;
		// amoga++;
		// local inner class
		// we cannot load it from inclosed class
		
		class Home{  
			// local inner class
			// we cannot laod it from inclosed
		public void run() {
			System.out.println("mxmx= "+mxmx);
		//	System.out.println("heyyy"+heyyy);
			System.out.println("gk= "+gk);
			System.out.println("amoga="+amoga);
			//System.out.println("pk= "+pk);
			
		}
		}
		
	}

}
